{
  "editor.cursorStyle": "line-thin",
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.fontFamily": "'Cascadia Code', monospace",
  "editor.renderIndentGuides": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [100, 120],
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,

  "eslint.enable": true,
  "eslint.packageManager": "yarn",

  "explorer.confirmDelete": true,

  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,

  "git.confirmSync": false,

  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "MesloLGLDZ Nerd Font Mono",
  "terminal.integrated.scrollback": 10000,

  "javascript.updateImportsOnFileMove.enabled": "always",

  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",

  "window.menuBarVisibility": "visible",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "terminal.background": "#252525",
    "terminal.foreground": "#eceff1",
    "terminal.ansiBlack": "#263238",
    "terminal.ansiGreen": "#8bc34a",
    "terminal.ansiYellow": "#f7c42a",
    "terminal.ansiBlue": "#2196f3",
    "terminal.ansiMagenta": "#9575cd",
    "terminal.ansiCyan": "#00bcd4",
    "terminal.ansiWhite": "#eceff1",
    "terminal.ansiRed": "#a54242",
    "terminal.ansiBrightBlack": "#617f8a",
    "terminal.ansiBrightRed": "#ec407a",
    "terminal.ansiBrightGreen": "#9ccc65",
    "terminal.ansiBrightYellow": "#ffb74d",
    "terminal.ansiBrightBlue": "#42a5f5",
    "terminal.ansiBrightMagenta": "#b39ddb",
    "terminal.ansiBrightCyan": "#26c6da",
    "terminal.ansiBrightWhite": "#ffffff"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.statusBar.feedback.visible": false,
  "workbench.colorTheme": "One Dark Pro Vivid",
}
